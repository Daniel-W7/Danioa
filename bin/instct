#!/bin/bash
#
echo "start to install tctconfig"
#获取运行的程序名
PRONAME=`basename $0`
#获取文件运行的当前目录
BINPATH=$(cd "$(dirname "$0")"; pwd)
touch /etc/profile.d/tctexport.sh
chmod +x /etc/profile.d/tctexport.sh
echo "#!/bin/bash
	export PATH=$BINPATH:$PATH">/etc/profile.d/tctexport.sh
echo "tctconfig has been installed successfully"
su - `whoami`
